{
  "name": "video-ai-ng",
  "version": "1.0.0",
  "description": "视频AI前端",
  "author": "dsunsoft",
  "homepage": "https://ng-alain.github.io/ng-alain/",
  "bugs": {
    "url": "https://github.com/ng-alain/ng-alain/issues"
  },
  "license": "MIT",
  "keywords": [
    "delon",
    "antd",
    "ng-zorro-antd",
    "angular",
    "component",
    "scaffold"
  ],
  "scripts": {
    "ng": "ng",
    "start": "npm run color-less && ng serve -o",
    "hmr": "npm run color-less && ng serve -c=hmr",
    "build": "npm run color-less && ng build --prod --build-optimizer",
    "prod": "node --max_old_space_size=6096 ./node_modules/@angular/cli/bin/ng build --prod",
    "analyze": "ng build --prod --build-optimizer --stats-json",
    "lint": "npm run lint:ts && npm run lint:style",
    "lint:ts": "tslint -p src/tsconfig.app.json -c tslint.json 'src/**/*.ts'",
    "lint:style": "stylelint \"{src}/**/*.less\" --syntax less",
    "lint-staged": "lint-staged",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "e2e": "ng e2e",
    "test": "ng test --watch",
    "test-coverage": "ng test --code-coverage --watch=false",
    "color-less": "node scripts/color-less.js",
    "icon": "ng g ng-alain:plugin icon"
  },
  "dependencies": {
    "@angular/animations": "~7.2.0",
    "@angular/common": "~7.2.0",
    "@angular/compiler": "~7.2.0",
    "@angular/core": "~7.2.0",
    "@angular/forms": "~7.2.0",
    "@angular/platform-browser": "~7.2.0",
    "@angular/platform-browser-dynamic": "~7.2.0",
    "@angular/router": "~7.2.0",
    "@antv/data-set": "^0.10.1",
    "@antv/g2": "^3.4.8",
    "@antv/g2-plugin-slider": "^2.1.1",
    "@delon/abc": "7.0.0-rc.6",
    "@delon/acl": "7.0.0-rc.6",
    "@delon/auth": "7.0.0-rc.6",
    "@delon/cache": "7.0.0-rc.6",
    "@delon/chart": "7.0.0-rc.6",
    "@delon/form": "7.0.0-rc.6",
    "@delon/mock": "7.0.0-rc.6",
    "@delon/theme": "7.0.0-rc.6",
    "@delon/util": "7.0.0-rc.6",
    "@ngx-translate/core": "^11.0.1",
    "@ngx-translate/http-loader": "^4.0.0",
    "@types/echarts": "^4.4.1",
    "ajv": "^6.6.1",
    "classlist.js": "^1.1.20150312",
    "core-js": "^2.5.4",
    "echarts": "^4.4.0",
    "file-saver": "^2.0.0",
    "intl": "^1.2.5",
    "jquery": "3.2.1",
    "ng-zorro-antd": "7.2.0",
    "ngx-color-picker": "7.5.0",
    "ngx-countdown": "^3.1.0",
    "ngx-echarts": "^4.2.1",
    "ngx-tinymce": "^7.0.0",
    "ngx-ueditor": "^2.1.3",
    "npm": "^6.9.0",
    "qrious": "^4.0.2",
    "rxjs": "~6.3.3",
    "screenfull": "^4.0.0",
    "tslib": "^1.9.0",
    "url-search-params-polyfill": "7.0.0",
    "web-animations-js": "^2.3.2",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.12.0",
    "@angular/cli": "~7.2.0",
    "@angular/compiler-cli": "~7.2.0",
    "@angular/language-service": "~7.2.0",
    "@angularclass/hmr": "^2.1.3",
    "@delon/testing": "7.0.0-rc.6",
    "@types/jasmine": "~2.8.8",
    "@types/jasminewd2": "~2.0.3",
    "@types/jszip": "^3.1.4",
    "@types/mockjs": "^1.0.2",
    "@types/node": "~8.9.4",
    "codecov": "^3.1.0",
    "codelyzer": "~4.5.0",
    "editorconfig-tools": "^0.1.1",
    "gh-pages": "^2.0.1",
    "husky": "^1.3.1",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~6.3.16",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.1",
    "karma-jasmine": "~1.1.2",
    "karma-jasmine-html-reporter": "^0.2.2",
    "less-bundle-promise": "^1.0.7",
    "lint-staged": "^8.1.0",
    "mockjs": "^1.0.1-beta3",
    "ng-alain": "7.0.0-rc.6",
    "ng-alain-codelyzer": "^0.0.1",
    "prettier": "^1.15.3",
    "prettier-stylelint": "^0.4.2",
    "protractor": "~5.4.0",
    "stylelint": "^9.9.0",
    "stylelint-config-standard": "^18.2.0",
    "ts-node": "~7.0.0",
    "tslint": "~5.11.0",
    "tslint-config-prettier": "^1.17.0",
    "tslint-language-service": "^0.9.9",
    "typescript": "~3.2.2",
    "webpack-bundle-analyzer": "^3.0.3",
    "xlsx": "^0.14.1"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint-staged"
    }
  },
  "lint-staged": {
    "*.{cmd,html,json,md,sh,txt,xml,yml}": [
      "editorconfig-tools fix",
      "git add"
    ],
    "*.ts": [
      "npm run lint:ts",
      "prettier --write",
      "git add"
    ],
    "*.less": [
      "npm run lint:style",
      "prettier --write",
      "git add"
    ],
    "ignore": [
      "src/assets/*"
    ]
  }
}
